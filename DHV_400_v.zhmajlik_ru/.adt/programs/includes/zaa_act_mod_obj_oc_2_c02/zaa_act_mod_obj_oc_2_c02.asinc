MODULE status_0200 OUTPUT.
  SET PF-STATUS '0100'.
  SET TITLEBAR '100'.
  lcl_report=>pbo_100( ).
ENDMODULE.

*&---------------------------------------------------------------------*
*&      Module  USER_COMMAND_0100  INPUT
*&---------------------------------------------------------------------*
*       text
*----------------------------------------------------------------------*
MODULE user_command_0100 INPUT.
  CASE sy-ucomm.
    WHEN 'BACK' OR 'CANCEL'.
      cl_excel=>destroy_excel( ).
      LEAVE TO SCREEN 0.
    WHEN 'EXIT'.
      cl_excel=>destroy_excel( ).
      LEAVE.
  ENDCASE.
ENDMODULE.