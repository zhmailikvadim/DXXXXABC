@EndUserText.label: 'ZSD_I_ARUNALLASSIGNMENTS access control'
@MappingRole: true
define role ZSD_I_ARUNALLASSIGNMENTS {
    grant
        select
            on
                ZSD_I_ARUNALLASSIGNMENTS
                    where
      (  
        Plant
       ) =
      aspect pfcg_auth ( arun_sdo,
                         werks );
}