@EndUserText.label: 'Access Control ZSD_I_HIGH_LVL_ITEM_GROUP'
@MappingRole: true
define role ZSD_I_HIGH_LVL_ITEM_GROUP {
    grant
        select
            on
                ZSD_I_CONF_QUANT_GROUP
                    where
                        inheriting conditions from entity I_SalesDocumentItem;
                
}