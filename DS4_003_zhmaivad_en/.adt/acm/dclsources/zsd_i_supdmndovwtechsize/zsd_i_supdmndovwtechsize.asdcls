@EndUserText.label: 'ZSD_I_SUPDMNDOVWTECHSIZE access control'
@MappingRole: true
define role ZSD_I_SUPDMNDOVWTECHSIZE {
    grant
        select
            on
                ZSD_I_SUPDMNDOVWTECHSIZE
                    where
      (  
        klart
       ) =
      aspect pfcg_auth ( C_TCLA_BKA,
                         klart );
}