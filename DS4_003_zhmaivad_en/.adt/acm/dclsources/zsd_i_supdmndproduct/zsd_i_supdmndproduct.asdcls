@EndUserText.label: 'ZSD_I_SUPDMNDPRODUCT access control'
@MappingRole: true
define role ZSD_I_SUPDMNDPRODUCT {
    grant
        select
            on
                ZSD_I_SUPDMNDPRODUCT
                    where
      (  
        mtart
       ) =
      aspect pfcg_auth ( K_ML_MTART,
                         mtart );
}