FUNCTION RH_GET_PLOG_TAB
  EXPORTING
    VALUE(PLOG_TAB_COUNT_EXP) TYPE ANY
    VALUE(TB_PLOG_TAB_COUNT_EXP) TYPE ANY
  TABLES
    PLOG_TAB_TAB LIKE HRDBTAB
    TB_PLOG_TAB_TAB LIKE HRTBUFFER OPTIONAL.




  PLOG_TAB_COUNT_EXP     = PLOG_TAB_COUNT.
  TB_PLOG_TAB_COUNT_EXP  = TB_PLOG_TAB_COUNT.

  CLEAR PLOG_TAB_TAB.
  PLOG_TAB_TAB[] = PLOG_TAB[].

  CLEAR TB_PLOG_TAB_TAB.
  TB_PLOG_TAB_TAB[] = TB_PLOG_TAB[].

ENDFUNCTION.
